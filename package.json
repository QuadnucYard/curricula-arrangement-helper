{
  "name": "curricula-arrangement-helper",
  "version": "0.1.0",
  "main": "dist/electron/main/index.js",
  "description": "Help you arrange your curricula in SEU.",
  "author": "QuadnucYard",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder --ia32"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.8",
    "vite-plugin-electron": "^0.10.2",
    "vue": "^3.2.37",
    "vue-tsc": "^0.40.1"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "axios": "^0.27.2",
    "bitset": "^5.1.1",
    "element-plus": "^2.2.13",
    "sass": "^1.54.4",
    "vite-plugin-commonjs-externals": "^0.1.1",
    "vue-axios": "^3.4.1",
    "vue-router": "^4.1.3",
    "vuex": "^4.0.2"
  },
  "config": {
    "arch": "ia32",
    "registry": "https://registry.npm.taobao.org"
  },
  "build": {
    "appId": "com.electron.desktop",
    "productName": "Curricula Arrangement Helper",
    "asar": true,
    "copyright": "Copyright Â© 2022 QuadnucYard",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist",
      "!node_modules/**",
      "node_modules/@electron"
    ]
  }
}
